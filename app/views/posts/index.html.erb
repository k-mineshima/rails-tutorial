<h1>All Posts</h1>
<%= notice %>
<%= alert %>
<div class="new-post">
  <%= link_to("New Post", new_post_path) %>
</div>
<div>
  <% @posts.each do |post| %>
    <%= link_to(post) do %>
      <div class="post">
        <div class="nickname">
          <%= post.nick_name %>
        </div>
        <div class="content">
          <%= post.content %>
        </div>
      </div>
    <% end %>
    <p>------------------------------------</p>
  <% end %>
</div>
